page{
  background-color: #dedede;
  padding: 20rpx;
}
.search_row{
  height: 60rpx;
  display: flex;
}
.search_row input{
  flex: 1;
  height: 100%;
  background-color: #fff;
}
.search_row .btn{
  width: 110rpx;
  height: 100%;
  font-size: 26rpx;
  padding: 0;
  margin-left: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #fff;
}

.search_content{
  margin-top: 30rpx;
}
.serch_item{
  background-color: #fff;
  font-size: 26rpx;
  padding: 15rpx 10rpx;
  border-bottom: 1rpx solid #ccc;

  overflow: hidden;
  white-space: normal;
  text-overflow: ellipsis;
}